@page "/"
@namespace Uploadify.Client.Api.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
    <title>Uploadify</title>
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet"/>
    <link href="~/icons/brand-logo.svg" rel="icon"/>
    <link href="~/icons/favicon_160x160.png" rel="icon" type="image/png" sizes="160x160"/>
    <link href="~/icons/favicon_180x180.png" rel="icon" type="image/png" sizes="180x180"/>
    <link href="~/icons/favicon_196x196.png" rel="icon" type="image/png" sizes="196x196"/>
    <link href="_content/MudBlazor/MudBlazor.min.css" rel="stylesheet"/>
    <link href="~/css/app.css" rel="stylesheet"/>
</head>
<body>
<div id="app">
    <div class="spinner"></div>
</div>
<div id="blazor-error-ui">
    <environment include="Staging,Production">
        An error has occurred. This application may no longer respond until reloaded.
    </environment>
    <environment include="Development">
        An unhandled exception has occurred. See browser dev tools for details.
    </environment>
</div>
<script src="~/js/app.js"></script>
<script src="_content/MudBlazor/MudBlazor.min.js"></script>
<script src="_framework/blazor.webassembly.js"></script>
<script src="~/js/antiForgeryToken.js"></script>
@Html.AntiForgeryToken()
</body>
</html>
